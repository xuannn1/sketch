    "cross-env": "^5.1.3",
    "jquery": "^3.1.1",
    "laravel-elixir-browsersync2": "^0.1.0",
    "laravel-mix": "^1.5.0",
    "lodash": "^4.17.4",
    "vue": "^2.5.16"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
